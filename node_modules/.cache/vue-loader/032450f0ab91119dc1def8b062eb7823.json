tent":["<template>\r\n  <v-data-table\r\n    v-model=\"search\"\r\n    :headers=\"headers\"\r\n    :items=\"credenciais\"\r\n    class=\"elevation-1\"\r\n    :search=\"search\"\r\n    :footer-props=\"{itemsPerPageText: 'Itens por página'}\"\r\n  >\r\n    <template v-slot:item.permission=\"{ item }\">\r\n      <v-chip-group\r\n          mandatory\r\n          active-class=\"primary--text\"\r\n        >\r\n      <v-chip\r\n        v-for=\"tag in tags\"\r\n        :key=\"tag\"\r\n      >\r\n        {{ tags }}\r\n      </v-chip>\r\n      </v-chip-group>\r\n    </template>\r\n    <template v-slot:top>\r\n        <v-toolbar\r\n          flat\r\n        >\r\n            <v-dialog\r\n              v-model=\"dialog\"\r\n              max-width=\"500px\"\r\n            >\r\n              \r\n              <v-card>\r\n                <v-card-title>\r\n                  <span class=\"headline\">{{ formTitle }}</span>\r\n                </v-card-title>\r\n\r\n                <v-card-text>\r\n                  <v-container>\r\n                    <v-row>\r\n                      <v-col\r\n                        cols=\"10\"\r\n                        sm=\"8\"\r\n                        md=\"6\"\r\n                      >\r\n                        <v-text-field\r\n                          v-model=\"editedItem.nome\"\r\n                          label=\"Nome\"\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                      <v-col\r\n                        cols=\"12\"\r\n                        sm=\"6\"\r\n                        md=\"4\"\r\n                      >\r\n                        <v-text-field\r\n                          v-model=\"editedItem.status\"\r\n                          label=\"Status\"\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                    </v-row>\r\n                    \r\n                  </v-container>\r\n                </v-card-text>\r\n                <template v-slot:item.permission=\"{ item }\">\r\n      <v-chip\r\n        :color=\"getColor(item.calories)\"\r\n        dark\r\n      >\r\n        {{ item.calories }}\r\n      </v-chip>\r\n    </template>\r\n                <v-card-actions>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn\r\n                    color=\"blue darken-1\"\r\n                    text\r\n                    @click=\"close\"\r\n                  >\r\n                    Cancelar\r\n                  </v-btn>\r\n                  <v-btn\r\n                    color=\"blue darken-1\"\r\n                    text\r\n                    @click=\"save\"\r\n                  >\r\n                    Salvar\r\n                  </v-btn>\r\n                </v-card-actions>\r\n              </v-card>\r\n            </v-dialog>\r\n            <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n              <v-card>\r\n                <v-card-title class=\"headline\">Você tem certeza que quer deletar?</v-card-title>\r\n                <v-card-actions>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Cancelar</v-btn>\r\n                  <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\">Sim!</v-btn>\r\n                  <v-spacer></v-spacer>\r\n                </v-card-actions>\r\n              </v-card>\r\n            </v-dialog>\r\n        </v-toolbar>\r\n    </template>\r\n    <template v-slot:item.actions=\"{ item }\">\r\n      <v-icon\r\n        small\r\n        class=\"mr-2\"\r\n        @click=\"editItem(item)\"\r\n      >\r\n        mdi-pencil\r\n      </v-icon>\r\n      <v-icon\r\n        small\r\n        @click=\"deleteItem(item)\"\r\n      >\r\n        mdi-delete\r\n      </v-icon>\r\n    </template>\r\n    <template v-slot:no-data>\r\n      <v-btn\r\n        color=\"primary\"\r\n        @click=\"initialize\"\r\n      >\r\n        Reset\r\n      </v-btn>\r\n    </template>\r\n  </v-data-